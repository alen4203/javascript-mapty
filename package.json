{
  "name": "mapty",
  "version": "1.0.0",
  "description": "An application for keeping exercise records in a map",
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "clean-dist": "rm -rf dist && mkdir dist",
    "copy-images": "cp -r ./images ./dist",
    "init": "npm run clean-dist && npm run copy-images",
    "start": "npm run init && parcel index.html",
    "build": "parcel build index.html"
  },
  "author": "CHENG-HSIANG, WANG",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.22.6",
    "leaflet": "^1.8.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "parcel": "^2.5.0"
  }
}
